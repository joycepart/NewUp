<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/indiana_win_plo"
    android:orientation="vertical">

<View
    android:layout_width="match_parent"
    android:layout_height="@dimen/ds_30"
    android:background="@color/color_cc"/>

        <RelativeLayout
            android:id="@+id/win_rel"
            android:layout_width="match_parent"
            android:layout_height="@dimen/ds_240">
    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_ff"
        android:gravity="center_vertical"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/indiana_plo"
            android:layout_marginLeft="@dimen/ds_30"
            android:gravity="center"
            android:textSize="@dimen/fs_45"
            android:textColor="@color/color_00"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/ds_40"
            android:layout_marginLeft="@dimen/ds_30"
            android:text="@string/indiana_add"
            android:gravity="center"
            android:textSize="@dimen/fs_45"
            android:textColor="@color/color_00"/>
    </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/ds_30"
                android:gravity="center"
                android:orientation="horizontal">
            <TextView
                android:id="@+id/win_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/add_choose"
                android:gravity="center"
                android:textSize="@dimen/fs_45"
                android:textColor="@color/color_00"/>

                <ImageView
                    android:layout_width="@dimen/ds_80"
                    android:layout_height="@dimen/ds_80"
                    android:background="@drawable/home_right_arrow_icon"/>
            </LinearLayout>
        </RelativeLayout>
<LinearLayout
    android:visibility="gone"
    android:id="@+id/win_lin"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_ff"
    android:orientation="horizontal">
    <LinearLayout

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">
        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="@dimen/ds_130"
            android:background="@color/color_ff"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/indiana_plo"
                android:layout_marginLeft="@dimen/ds_30"
                android:gravity="center"
                android:textSize="@dimen/fs_45"
                android:textColor="@color/color_00"/>
            <TextView
                android:id="@+id/indiana_win_plo_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/ds_30"
                android:text=""
                android:gravity="center"
                android:textSize="@dimen/fs_45"
                android:textColor="@color/color_00"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/indiana_win_add"
            android:layout_width="match_parent"
            android:layout_height="@dimen/ds_130"
            android:background="@color/color_ff"
          >
            <TextView
                android:id="@+id/indiana_win_add_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/indiana_add"
                android:layout_marginLeft="@dimen/ds_30"
                android:gravity="center"
                android:textSize="@dimen/fs_45"
                android:textColor="@color/color_00"/>
            <TextView
                android:id="@+id/win_num_time"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/ds_30"
                android:layout_marginLeft="@dimen/ds_30"
                android:maxLines="1"
                android:ellipsize="end"
                android:text=""
                android:gravity="center"
                android:textSize="@dimen/fs_45"
                android:textColor="@color/color_00"/>
        </RelativeLayout>
    </LinearLayout>
    <ImageView
        android:layout_width="@dimen/ds_80"
        android:layout_height="@dimen/ds_80"
        android:layout_gravity="center"
        android:layout_marginRight="@dimen/ds_30"
        android:background="@drawable/home_right_arrow_icon"/>
</LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/ds_1"
            android:background="@color/color_cc"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/ds_130"
            android:background="@color/color_ff"
            android:orientation="horizontal">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/indiana_mode"
                android:layout_marginLeft="@dimen/ds_30"
                android:gravity="center"
                android:textSize="@dimen/fs_45"
                android:textColor="@color/color_00"/>
            <TextView
                android:id="@+id/det_data_user"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/ds_30"
                android:layout_marginLeft="@dimen/ds_20"
                android:text="@string/deliver_no"
                android:gravity="center"
                android:textSize="@dimen/fs_45"
                android:textColor="@color/color_00"/>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/ds_130"
            android:background="@color/color_ff"
            android:orientation="horizontal">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/indiana_numn"
                android:layout_marginLeft="@dimen/ds_30"
                android:gravity="center"
                android:textSize="@dimen/fs_45"
                android:textColor="@color/color_00"/>
            <TextView
                android:id="@+id/det_data_nm"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/ds_30"
                android:layout_marginLeft="@dimen/ds_20"
                android:text="@string/deliver_no"
                android:gravity="center"
                android:textSize="@dimen/fs_45"
                android:textColor="@color/color_00"/>
            </RelativeLayout>
        </LinearLayout>
    <Button
        android:id="@+id/win_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_click"
        android:text="@string/add_confirm"
        android:textSize="@dimen/fs_55"
        android:textColor="@color/color_ff"
        android:layout_marginTop="@dimen/ds_70"
        android:layout_marginLeft="@dimen/ds_60"
        android:layout_marginRight="@dimen/ds_60"/>
</LinearLayout>
